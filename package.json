{
    "name": "autoconf",
    "displayName": "autoconf",
    "description": "Syntax support for the Autoconf M4 and Automake files (Autotools)",
    "version": "0.0.4",
    "icon": "icon.png",
    "publisher": "maelvalais",
    "repository": "https://github.com/maelvalais/vscode_autotools",
    "license": "MIT",
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "m4",
                "aliases": [
                    "Autoconf M4",
                    "m4"
                ],
                "extensions": [
                    "configure.ac",
                    "configure.in",
                    ".m4"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "automake",
                "aliases": [
                    "Automake",
                    "automake"
                ],
                "extensions": [
                    ".am",
                    ".am.inc",
                    "Makefile.in"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "m4",
                "scopeName": "source.m4",
                "path": "./Autoconf.tmLanguage"
            },
            {
                "language": "automake",
                "scopeName": "source.automake",
                "path": "./Automake.tmLanguage"
            }
        ]
    },
    "devDependencies": {
        "plist": "latest",
        "glob-watcher": "^4.0.0"
    },
    "scripts": {
        "start": "node build.js -w",
        "compile": "node build.js"
    }
}
